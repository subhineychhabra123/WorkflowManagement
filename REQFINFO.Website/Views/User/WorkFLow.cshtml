@{
    ViewBag.Title = "WorkFLow";
    Layout = "~/Views/Shared/_InnerLayout.cshtml";
}
<script src="~/Scripts/Controllers/WorkflowController.js"></script>

<div class="main-content" ng-app="workflowApp">
    <div class="container" ng-controller="WorkflowCtlr">
        <div class="row">
            <div class="col-lg-12 col-md-12 col-sm-12 col-xs-12">
                <div class="page-content">
                    <div class="theme-title">
                        <div class="theme-title-border">
                        <i class="fa fa-clipboard"></i><span>WorkFLow</span>
                    </div>
                        </div>
                    <div class="white-area">
                        <div class="row">


                            <div ng-if="workflow.length === 0" class="col-lg-4 col-md-4 col-sm-6 col-xs-12">

                                <span class="Emptymsg">No Record Found  </span>
                            </div>
                        </div>




                        <div ng-repeat="workflowObj in workflow" class="col-lg-4 col-md-4 col-sm-6 col-xs-12">
                           <a  href="/Projects/{{workflowObj.IDWorkflow}}">
                             <div class="panel-project"   >
                                <div class="fa large-icon fa-clipboard"></div>
                                <div class="panel-title">
                                    <i class="fa fa-clipboard"></i>
                                    <span>{{workflowObj.Name}} </span>
                                    <div >
                                    
                                     </div>
                                </div>
                                <div class="panel-content">
                                </div>
                            </div>
                               </a>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>

